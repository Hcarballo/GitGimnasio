
@{
    ViewData["Title"] = "Ingresos";
}


@using (Html.BeginForm("Ingresos", "Socios", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="container">
        <div class="form-horizontal">
            <div class="form-group">
                <input type="text" name="dni" id="dni" value="" style="background-color:antiquewhite" />
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">                       
                    <input type="submit" value="Aceptar" class="btn btn-success" />
                </div>
            </div>
        </div>
    </div>
}
