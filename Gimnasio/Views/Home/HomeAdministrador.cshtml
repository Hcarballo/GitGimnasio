
@{
    ViewData["Title"] = "HomeAdministrador";
}

<link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
<link href="~/css/site.css" rel="stylesheet" type="text/css" />

<h2>HomeAdministrador</h2>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <p>
                <a href="~/ApplicationUsers/Index" class="btn btn-sq-lg btn-primary" style="margin-right:5px">
                    <i class="fa fa-user fa-5x"></i><br />
                    Usuarios <br>
                </a>
                <a href="#" class="btn btn-sq-lg btn-success" style="margin-right:5px">
                    <i class="fa fa-user fa-5x"></i><br />
                    Profesores <br>
                </a>
                <a href="~/Cuotas/Index" class="btn btn-sq-lg btn-info" style="margin-right:5px">
                    <i class="fa fa-user fa-5x"></i><br />
                    Cuotas<br>
                </a>
                <a id="btnNuevo" data-toggle="modal" data-target="#myModal" class="btn btn-sq-lg btn-warning" style="margin-right:5px">
                    <i class="fa fa-user fa-5x"></i><br />
                    Ingreso Socio<br>
                </a>
                    <a href="~/Socios/Index" class="btn btn-sq-lg btn-danger">
                        <i class="fa fa-user fa-5x"></i><br />
                        Socios <br>
                    </a>
</p>
        </div>
    </div>
</div>


<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">  
                <img src="~/images/Logo_Gym.jpg" style="width:40px; height:40px" class="img-responsive" />
            </div>
            <div class="modal-body">
                <h4>Ingresar DNI</h4>
                <input type="text" name="dni" id="dni"/>
            </div>
            <div class="modal-footer">
                
                <button type="button" class="btn btn-success" onclick="javascript: enviar();">ACEPTAR</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <img src="~/images/Logo_Gym.jpg" style="width:40px; height:40px" class="img-responsive" />
            </div>
            <div class="modal-body">
                <h4>DNI</h4>
                <input type="text" name="dni" id="dni" />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success" onclick="javascript: enviar();">ACEPTAR</button>
            </div>
        </div>
    </div>
</div>



@section scripts
    {
    <script src="~/js/site.js"></script>
}





